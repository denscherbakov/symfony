{% extends 'admin/base.html.twig' %}

{% block body %}

    <div class="container-fluid">
        <div class="row no-gutters">
            <div class="col-12">
                <h1 class="display-4 text-center mt-5">{{ title }}</h1>
                <ul>
                    {% for user in users %}
                        <li>{{ user.email }} {{ user.roles | json_encode }}</li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>

{% endblock %}