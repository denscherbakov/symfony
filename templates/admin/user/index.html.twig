{% extends 'admin/base.html.twig' %}

{% block body %}

    <h1>{{ title }}</h1>

    <h2>Users list</h2>

    <a href="{{ path('admin/users/create') }}">Create user</a>

    <ul>
        {% for user in users %}
        <li>{{ user.email }} {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>

    <a href="{{ path('app_logout') }}">Exit</a>

{% endblock %}